<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RecordView">
        <attr name="roundSize" format="dimension" />
        <attr name="backgroundColor" format="color|reference" />
        <attr name="strokeWidth" format="dimension" />
    </declare-styleable>

    <declare-styleable name="AutoScaleRelativeLayout">
        <attr name="scaleRate" format="float" />
    </declare-styleable>

    <declare-styleable name="SwipeFlingAdapterView">
        <attr name="SwipeFlingStyle" format="reference" />
        <attr name="rotation_degrees" format="float" />
        <attr name="min_adapter_stack" format="integer" />
        <attr name="max_visible" format="integer" />
    </declare-styleable>
</resources>